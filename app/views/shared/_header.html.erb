<div id="hidden-menu">
  <button><%= icon('times') %></button>
  <ul>
    <li><%= link_to "Home", root_path %></li>
    <% if logged_in? %>
      <li><%= link_to "Log Out", logout_path, method: :delete %></li>
    <% else %>
      <li><%= link_to "Log In", login_path %></li>
    <% end %>
  </ul>
</div>

<%= link_to root_path do %>
  <%= image_tag "scoreboard-text200.png", alt: "Logo Image", class: "title-image hide-for-small-only" %>
  <%= image_tag "got-next-text50.png", alt: "Logo Image", class: "title-image show-for-small-only" %>
<% end %>

<div class="blocker hide-for-small-only"></div>

<nav id="tab-profile">
<% if @extra %>
  <%= render "shared/header/three_tab" %>
<% else %>
  <%= render "shared/header/two_tab" %>
<% end %>
</nav>


